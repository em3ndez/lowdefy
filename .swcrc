{
  "exclude": [".*.test.js$", ".*/tests/.*", ".*/__mocks__/.*"],
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "jsx": true,
      "dynamicImport": true,
      "topLevelAwait": true
    },
    "target": "es2020",
    "keepClassNames": true,
    "transform": { "react": { "runtime": "classic" } }
  },
  "module": {
    "type": "es6"
  }
}
